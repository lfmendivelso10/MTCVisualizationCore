<?xml version="1.0" encoding="UTF-8"?>
<pnet:System xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pnet="sistemas.uniandes.edu.co/metamodels/petrinet">
  <petrinets name="PNet1">
    <elements xsi:type="pnet:Transition" name="T1"/>
    <elements xsi:type="pnet:Place" name="P1"/>
    <elements xsi:type="pnet:Transition" name="T2"/>
    <elements xsi:type="pnet:OutputArc" to="//@petrinets.0/@elements.1" from="//@petrinets.0/@elements.0"/>
    <elements xsi:type="pnet:InputArc" to="//@petrinets.0/@elements.2" from="//@petrinets.0/@elements.1"/>
    <elements xsi:type="pnet:OutputArc" to="//@petrinets.1/@elements.0" from="//@petrinets.0/@elements.2"/>
  </petrinets>
  <petrinets name="PNet2">
    <elements xsi:type="pnet:Place" name="A1"/>
    <elements xsi:type="pnet:Transition" name="T3"/>
    <elements xsi:type="pnet:InputArc" to="//@petrinets.1/@elements.1" from="//@petrinets.1/@elements.0"/>
    <elements xsi:type="pnet:OutputArc" to="//@petrinets.2/@elements.0" from="//@petrinets.1/@elements.1"/>
  </petrinets>
  <petrinets name="PNet3">
    <elements xsi:type="pnet:Place" name="B1"/>
    <elements xsi:type="pnet:Transition" name="T4"/>
    <elements xsi:type="pnet:InputArc" to="//@petrinets.2/@elements.1" from="//@petrinets.2/@elements.0"/>
  </petrinets>
  <petrinetrelationships from="//@petrinets.0" to="//@petrinets.1"/>
  <petrinetrelationships from="//@petrinets.1" to="//@petrinets.2"/>
</pnet:System>
